<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatefulLayout">
        <attr name="sl_errorView" format="reference" />
        <attr name="sl_loadingView" format="reference" />
        <attr name="sl_emptyView" format="reference" />
        <attr name="sl_layoutState" format="enum">
            <enum name="empty" value="0" />
            <enum name="loading" value="1" />
            <enum name="error" value="2" />
            <enum name="content" value="3" />
        </attr>
    </declare-styleable>
</resources>